        <fullName>EventDates__c</fullName>
        <externalId>false</externalId>
        <formula>CASE(MONTH(DATEVALUE(EventStartDate__c)), 1,&apos;Jan&apos;, 2,&apos;Feb&apos;, 3,&apos;Mar&apos;, 4,&apos;Apr&apos;, 5,&apos;May&apos;, 6,&apos;Jun&apos;, 7,&apos;Jul&apos;, 8,&apos;Aug&apos;, 9,&apos;Sep&apos;, 10,&apos;Oct&apos;, 11,&apos;Nov&apos;, &apos;Dec&apos;)
 +&apos; &apos;+
 TEXT(DAY(DATEVALUE(EventStartDate__c)))
 +&apos;, &apos;+
 TEXT(YEAR(DATEVALUE(EventStartDate__c)))
 + 
 IF(EventStartDate__c != EventEndDate__c
 , &apos; - &apos; + 
 CASE(MONTH(DATEVALUE(EventEndDate__c)), 1,&apos;Jan&apos;, 2,&apos;Feb&apos;, 3,&apos;Mar&apos;, 4,&apos;Apr&apos;, 5,&apos;May&apos;, 6,&apos;Jun&apos;, 7,&apos;Jul&apos;, 8,&apos;Aug&apos;, 9,&apos;Sep&apos;, 10,&apos;Oct&apos;, 11,&apos;Nov&apos;, &apos;Dec&apos;)
 +&apos; &apos;+
 TEXT(DAY(DATEVALUE(EventEndDate__c)))
 +&apos;, &apos;+
 TEXT(YEAR(DATEVALUE(EventEndDate__c)))
 ,&apos;&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
