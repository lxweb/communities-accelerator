    <validationRules>
        <fullName>EventEndDate</fullName>
        <active>true</active>
        <description>When Event End Date is before than Event Start Date</description>
        <errorConditionFormula>EventEndDate__c &lt; EventStartDate__c</errorConditionFormula>
        <errorDisplayField>EventEndDate__c</errorDisplayField>
        <errorMessage>Can not be before than Event Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HTMLContentAndLongDescription</fullName>
        <active>false</active>
        <description>Validation on HTMLContent__c and LongDescription__c fields</description>
        <errorConditionFormula>AND(NOT(ISBLANK(HTMLContent__c)), LEN(Body__c) &lt;&gt; 0)</errorConditionFormula>
        <errorMessage>Can not fill both HTML Content and Long Description fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OverlayQuadrantObligatory</fullName>
        <active>true</active>
        <description>RecordType &quot;Banner&quot;; Cuando OverlayQuadrant__c este vaci y los campos Title__c, Extract__c, ButtonLabel__c tienen valor u OverlayQuadrant__c tiene valor y todos los campos Title__c, Extract__c, ButtonLabel__c  estn todos vacos.</description>
        <errorConditionFormula>AND(
    OR(
        AND(ISBLANK(TEXT(OverlayQuadrant__c)), OR(NOT(ISBLANK(Title__c)), NOT(LEN(Extract__c) = 0), NOT(ISBLANK(ButtonLabel__c))))  
    ,
        AND(NOT(ISBLANK(TEXT(OverlayQuadrant__c))), AND(ISBLANK(Title__c), LEN(Extract__c) = 0, ISBLANK(ButtonLabel__c)))  
    ),
                 RecordType.Name = &quot;Banner&quot;
)</errorConditionFormula>
        <errorDisplayField>OverlayQuadrant__c</errorDisplayField>
        <errorMessage>Si este campo esta vaco, los campos &quot;List Title&quot;, &quot;List Description&quot;, &quot;Button Label&quot; NO deben tener valor. 
Si este campo tiene valor, de los campos &quot;List Title&quot;, &quot;List Description&quot;, &quot;Button Label&quot; al menos uno NO debe estar vaco.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PublishEndDate</fullName>
        <active>true</active>
        <description>When Publish End Date is before than Publish Start Date</description>
        <errorConditionFormula>PublishEndDate__c &lt; PublishStartDate__c</errorConditionFormula>
        <errorDisplayField>PublishEndDate__c</errorDisplayField>
        <errorMessage>Can not be before than Publish Start Date</errorMessage>
    </validationRules>
