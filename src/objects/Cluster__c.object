    <fields>
        <fullName>ClusterLevel__c</fullName>
        <description>Represents the level of the cluster on the &quot;tree&quot;</description>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(ParentCluster__c)
,1
,IF(ISBLANK(ParentCluster__r.ParentCluster__c)
,2
,IF(ISBLANK(ParentCluster__r.ParentCluster__r.ParentCluster__c)
,3
,4
)
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Represents the level of the cluster on the &quot;tree&quot;</inlineHelpText>
        <label>Cluster Level</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ClusterRootExternalId__c</fullName>
        <externalId>false</externalId>
        <formula>if(RecordType.DeveloperName = &apos;ClusterRoot&apos;, ExternalId__c, &apos;&apos;) 
+
if(ParentCluster__r.RecordType.DeveloperName = &apos;ClusterRoot&apos;, ParentCluster__r.ExternalId__c, &apos;&apos;) 
+ 
if(ParentCluster__r.ParentCluster__r.RecordType.DeveloperName = &apos;ClusterRoot&apos;, ParentCluster__r.ParentCluster__r.ExternalId__c, &apos;&apos;) 
+ 
if(ParentCluster__r.ParentCluster__r.ParentCluster__r.RecordType.DeveloperName = &apos;ClusterRoot&apos;, ParentCluster__r.ParentCluster__r.ParentCluster__r.ExternalId__c, &apos;&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
