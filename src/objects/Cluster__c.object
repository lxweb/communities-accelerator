    <validationRules>
        <fullName>Avoid_Circular_References</fullName>
        <active>true</active>
        <description>Assure that child clusters don´t become parent of their parents</description>
        <errorConditionFormula>AND(RecordType.DeveloperName == &quot;ClusterGroup&quot;,NOT( OR(ParentCluster__r.RecordType.DeveloperName == &quot;ClusterRoot&quot;, ParentCluster__r.ParentCluster__r.RecordType.DeveloperName == &quot;ClusterRoot&quot;)))</errorConditionFormula>
        <errorMessage>Cluster tree must begin with a Root cluster (Avoid circular references)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cluster_Level_Must_Be_4_Or_Less</fullName>
        <active>true</active>
        <description>Cluster Depth Level Must Be Less Or Equat to 4</description>
        <errorConditionFormula>AND( RecordType.DeveloperName &lt;&gt; &quot;ClusterRoot&quot;, ParentCluster__r.ClusterLevel__c &gt; 3)</errorConditionFormula>
        <errorMessage>Maximum depth of cluster levels is four. With this parent cluster you are exceding the threshold</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Leaf_Cluster_Must_Have_Parent_Cluster</fullName>
        <active>true</active>
        <description>Every Leaf cluster requieres a Parent Cluster</description>
        <errorConditionFormula>AND( RecordType.DeveloperName == &quot;ClusterLeaf&quot;,ISBLANK( ParentCluster__c ))</errorConditionFormula>
        <errorMessage>A Leaf Cluster requires a Parent Cluster</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Root_Cluster_Dont_Allow_Parent_Cluster</fullName>
        <active>true</active>
        <description>Root clusters must not have parent clusters</description>
        <errorConditionFormula>AND(RecordType.DeveloperName == &quot;ClusterRoot&quot;,  NOT(ISBLANK(ParentCluster__c  ) ) )</errorConditionFormula>
        <errorMessage>Root type cluster shouldn´t have parent cluster</errorMessage>
    </validationRules>
